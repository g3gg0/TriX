<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="UI"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="UI"
			DisplayName="Compile QT UserInterface File (*.ui)"
			CommandLine="$(QTDIR)/bin/uic.exe &quot;$(InputPath)&quot; -o ..\..\src\ui-qt\ui_$(InputName).h"
			Outputs="..\..\src\ui-qt\ui_$(InputName).h"
			FileExtensions="*.ui"
			ExecutionDescription="Creating UI File ..\..\src\ui-qt\ui_$(InputName).h"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
